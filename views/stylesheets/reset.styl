html, body, div, span,
h1, h2, h3, p, a, strong, ul, ol, li,
article, aside, header, menu, nav, section
	margin 0
	padding 0
	border 0
	font-size 100%
	font inherit
	vertical-align baseline

article, aside, header, menu, nav, section
	display block

body
	line-height 1.6

ul, ol
	padding-left 24px

ul
	list-style disc